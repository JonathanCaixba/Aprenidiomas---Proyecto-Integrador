{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/about_us",
      "destination": "/src/views/about_us"
    },
    {
      "source": "/courses",
      "destination": "/src/views/courses"
    },
    {
      "source": "/community",
      "destination": "/src/views/community"
    },
    {
      "source": "/contact_us",
      "destination": "/src/views/contact_us"
    },
    {
      "source": "/signup",
      "destination": "/src/views/signup"
    },
    {
      "source": "/work-with-us",
      "destination": "/src/views/work-with-us"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}