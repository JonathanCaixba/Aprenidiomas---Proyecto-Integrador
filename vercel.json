{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/about.us",
      "destination": "/src/views/about_us"
    },
    {
      "source": "/community",
      "destination": "/src/views/community"
    },
    {
      "source": "/contact.us",
      "destination": "/src/views/contact.us"
    },
    {
      "source": "/signup",
      "destination": "/src/views/signup"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}